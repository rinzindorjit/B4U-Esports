{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "pages/api/approve-payment.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/api/complete-payment.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/approve-payment",
      "dest": "/pages/api/approve-payment.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/complete-payment",
      "dest": "/pages/api/complete-payment.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/pages/$1"
    }
  ],
  "env": {
    "PI_APP_URL": "https://b4u-esports.vercel.app",
    "PI_API_KEY": "@pi_api_key"
  }
}